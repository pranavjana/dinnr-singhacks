# MAS AML/CFT Crawler Configuration

# Download directory for PDFs
MAS_DOWNLOAD_DIR=./downloads

# HTTP request timeout (seconds)
MAS_REQUEST_TIMEOUT=10

# PDF download timeout (seconds)
MAS_PDF_TIMEOUT=30

# Maximum PDF file size (MB)
MAS_MAX_PDF_SIZE_MB=50

# Maximum retry attempts for failed requests
MAS_RETRY_ATTEMPTS=3

# User-Agent header for HTTP requests
MAS_USER_AGENT=DINNR-AML-Crawler/0.1.0 (+https://github.com/dinnr/singhacks)

# Logging level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
MAS_LOG_LEVEL=INFO
